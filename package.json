{
  "name": "yamp",
  "version": "0.3.10",
  "description": "Yet Another Markdown Parser",
  "main": "index.js",
  "bin": {
    "yamp": "./bin/yamp_cli.js"
  },
  "scripts": {
    "start": "node ./bin/yamp_cli.js",
    "test": "istanbul cover _mocha",
    "docs": "npm start -- docs/docs.md --html --pdf -o docs/index --koala; npm start -- docs/presentation.md --remark -o docs/presentation --koala; npm start -- README.md -o docs/readme --html --pdf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angrykoala/yamp.git"
  },
  "keywords": [
    "markdown",
    "parser",
    "html",
    "pdf",
    "yamp",
    "md"
  ],
  "author": "angrykoala <demiurgosoft@hotmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/angrykoala/yamp/issues"
  },
  "homepage": "https://github.com/angrykoala/yamp#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "ejs": "^2.5.2",
    "highlight.js": "^9.6.0",
    "html-minifier": "^3.0.3",
    "html-pdf": "^2.1.0",
    "markdown-it": "^8.0.0",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-toc": "^0.12.16",
    "pkginfo": "^0.4.0",
    "xejs": "^0.3.0"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "async": "^2.0.1",
    "chai": "^3.5.0",
    "fs-extra": "^0.30.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2"
  }
}
